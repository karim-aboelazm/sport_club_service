<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_partner_inherit_view_view_type_view_inherit" model="ir.ui.view">
            <field name="name">res.partner.inherit.view.view_type.view.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[last()]" position="after">
                    <page string="Sport Club" name="sport_club_page">
                        <group>
                            <field name="is_player"/>
                            <field name="is_trainer"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>

        <record id="contacts.action_contacts" model="ir.actions.act_window">
            <field name="domain">[('type', '!=', 'delivery')]</field>
        </record>
    </data>
</odoo>
