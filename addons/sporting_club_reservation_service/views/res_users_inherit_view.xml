<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_users_inherit_view_view_type_view_inherit" model="ir.ui.view">
            <field name="name">res.users.inherit.view.view_type.view.inherit</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[@name='preferences']//group[@name='preferences']" position="after">
                    <group string="More Information">
                        <field name="is_club_owner" widget="boolean_toggle"/>
                    </group>
                </xpath>
                <xpath expr="//page[last()]" position="after">
                    <page name="access_token" string="Access Tokens" groups="base.group_erp_manager">
                        <field name="access_token_ids">
                            <list edit="0" delete="1" create="0">
                                <field name="token" />
                                <field name="create_date" string="Issued At" />
                                <field name="expires" string="Expires At" />
                                <field name="is_expired" string="Expired" />
                            </list>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>